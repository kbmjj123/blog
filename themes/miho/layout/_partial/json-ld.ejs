<% if(is_home()){ %>
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Blog",
      "headline": "<%= page.title || config.title %>",
      "publisher": {
        "@type": "Person",
        "@id": "https://91temaichang.com",
        "name": "online阳光",
        "logo": {
            "@type": "ImageObject",
            "@id": "https://www.91temaichang.com/images/banner.jpg",
            "url": "https://www.91temaichang.com/images/banner.jpg",
            "width": "600",
            "height": "60"
        }
      },
      "keywords": "<%= config.keywords %>", 
      "url": "<%= config.url %>",
      "description": "<%= config.description %>",
      "articleBody": "<%= config.indexDesc %>",
      "author": {
        "@type": "Person",
        "name": "<%= config.author %>",
        "url": "https://www.91temaichang.com/about/",
        "jobTitle": "Programmer",
        "email": "<%= config.email %>",
        "image": {
          "@type": "ImageObject",
          "@id": "<%= config.userIcon %>",
          "url": "<%= config.userIcon %>",
          "height": "96",
          "width": "96"
        }
      }
    }
  </script>
<% }else if(is_post()){ %>
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "BlogPosting",
      "headline": "<%= page.title || config.title %>",
      "publisher": "https://www.91temaichang.com",
      "keywords": "<%= page.tags && page.tags.map(item => item.name).join(',') %>", 
      "image": "http://example.com/image.jpg",
      "url": "<%= page.permalink %>",
      "dateCreated": "<%= page.date %>",
      "dateModified": "<%= page.updated %>",
      "description": "<%= page.description %>",
      "articleBody": "<%= page.text %>",
      "author": {
        "@type": "Person",
        "name": "<%= config.author %>",
        "url": "https://www.91temaichang.com/about/",
        "jobTitle": "Programmer",
        "email": "<%= config.email %>",
        "image": {
          "@type": "ImageObject",
          "@id": "<%= config.userIcon %>",
          "url": "<%= config.userIcon %>",
          "height": "96",
          "width": "96"
        }
      }
    }
  </script>
<% } %>